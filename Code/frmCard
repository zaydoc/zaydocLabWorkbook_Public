        Sheets("WorkingData").Range("CH" & r) = D3R1Dormant.Text
        Sheets("WorkingData").Range("CI" & r) = D3R2Dormant.Text
        Sheets("WorkingData").Range("CJ" & r) = D3R3Dormant.Text
        Sheets("WorkingData").Range("CK" & r) = D3R4Dormant.Text
        Sheets("WorkingData").Range("CL" & r) = D3R1Hard.Text
        Sheets("WorkingData").Range("CM" & r) = D3R2Hard.Text
        Sheets("WorkingData").Range("CN" & r) = D3R3Hard.Text
        Sheets("WorkingData").Range("CO" & r) = D3R4Hard.Text
