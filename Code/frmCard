        Else
            D4R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DR" & r).Value > 0 Then
            D5R1Total.Text = Sheets("WorkingData").Range("DR" & r)
        Else
            D5R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DS" & r).Value > 0 Then
            D5R2Total.Text = Sheets("WorkingData").Range("DS" & r)
        Else
            D5R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DT" & r).Value > 0 Then
            D5R3Total.Text = Sheets("WorkingData").Range("DT" & r)
        Else
            D5R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DU" & r).Value > 0 Then
            D5R4Total.Text = Sheets("WorkingData").Range("DU" & r)
        Else
            D5R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ER" & r).Value > 0 Then
            D6R1Total.Text = Sheets("WorkingData").Range("ER" & r)
        Else
            D6R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ES" & r).Value > 0 Then
            D6R2Total.Text = Sheets("WorkingData").Range("ES" & r)
        Else
            D6R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ET" & r).Value > 0 Then
            D6R3Total.Text = Sheets("WorkingData").Range("ET" & r)
        Else
            D6R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("EU" & r).Value > 0 Then
            D6R4Total.Text = Sheets("WorkingData").Range("EU" & r)
        Else
            D6R4Total.Text = ""
        End If
        
        CurrentCard = RowNumber.Value 'Saves user's place in cards for when they return
        
        'Turns back off worksheets this form needed
        Sheets("WorkingData").Visible = False
        Application.ScreenUpdating = True
        
    Else
        
        'Alert the user that the value is illegal and reset RowNumber to the previous value
        MsgBox "Illegal record number. This value must be a number between 1 and " & LastRowWorkingData & "."
        RowNumber.Value = r - 1
    
    End If

End Sub

Private Sub ButtonFirst_Click()

    'Moves RowNumber value to first record in the worksheet and pulls first record data
    RowNumber.Value = 1

End Sub

Private Sub ButtonLast_Click()
 
    'Moves RowNumber value to last record in the worksheet and pulls last record data
    RowNumber.Value = LastRowWorkingData
    
End Sub

Private Sub ButtonPrevious_Click()

    'Moves RowNumber value down one if a valid record exists for that row in the worksheet
    
    If RowNumber.Value - 1 > 1 Then
        
        'Imports data from next row of WorkingData worksheet into CardData form
        RowNumber.Value = RowNumber.Value - 1
        r = RowNumber.Value + 1
    
    ElseIf RowNumber.Value - 1 <= 1 Then
        RowNumber.Value = 1
        r = RowNumber.Value + 1
        
    Else
        MsgBox "An error has occurred. Cannot move to next record in dataset."
        
    End If

End Sub

Private Sub ButtonNext_Click()
 
    'Moves RowNumber value up one if a valid record exists for that row in the worksheet
    
    If RowNumber.Value + 1 <= LastRowWorkingData Then
    
        'Imports data from next row of WorkingData worksheet into CardData form
        RowNumber.Value = RowNumber.Value + 1
        r = RowNumber.Value + 1

    ElseIf RowNumber.Value + 1 > LastRowWorkingData Then
        RowNumber.Value = LastRowWorkingData
        r = LastRowWorkingData + 1
        
    Else
        MsgBox "An error has occurred. Cannot move to next record in dataset."
        
    End If
    
End Sub

Private Sub ButtonSubmitSearch_Click()

    'Find first instance in WorkingData of the value in SearchBox
    Dim found As Range
    
    'Turns on worksheets that this procedure will need
    Application.ScreenUpdating = False
    Sheets("WorkingData").Visible = True
    
    'Searches for the next P.I. in WorkingData containing the searchbox value
    If SearchComboBox.Value = "PI" Then
        Set found = Sheets("WorkingData").Columns("B").Find(What:=SearchBox.Value, LookIn:=xlValues, LookAt:=xlPart, SearchOrder:=xlByRows, SearchDirection:=xlNext, MatchCase:=False, SearchFormat:=False)
        If found Is Nothing Then
            MsgBox "No results found."
        Else
            RowNumber.Value = found.Row - 1
        End If
    End If

    'Turns back off worksheets this procedure needed
    Sheets("WorkingData").Visible = False
    Application.ScreenUpdating = True

End Sub

Private Sub ButtonClearSearch_Click()

    'Clears value of Search textbox
    SearchBox.Text = ""

End Sub

