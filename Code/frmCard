    Sheets("Technician_Export").Visible = False
    Sheets("ProgramStorage").Visible = False
    Application.ScreenUpdating = True
    
    'Saves Workbook and alerts the user all operations were successful
        'Hide all worksheets in workbook except "Start" to make sure macros are enabled
        Dim ws As Worksheet
        Sheets("START").Visible = xlSheetVisible 'Make sure the "Start" worksheet is visible
 
        'Hide all other worksheets
        For Each ws In ThisWorkbook.Worksheets
            If ws.Name <> "START" Then
                ws.Visible = xlVeryHidden
            End If
        Next ws
    
        'Save Workbook
        ActiveWorkbook.Save
    
        'Turn FormBG back on once macros are enabled for workbook
        Sheets("FormBG").Visible = True
        Sheets("START").Visible = xlVeryHidden
        Application.ScreenUpdating = True

End Sub

Private Sub ButtonSaveClose_Click()

    Dim RepCount As Integer
    
    'Turns on worksheets that this form will need
    Application.ScreenUpdating = False
    Sheets("WorkingData").Visible = True
    Sheets("RESOURCEDATABASE_Export").Visible = True
    Sheets("Technician_Export").Visible = True
    Sheets("ProgramStorage").Visible = True
    
    'Saves data currently stored in form back to WorkingData worksheet
 
    r = RowNumber.Value + 1
    Sheets("ProgramStorage").Range("C32") = CurrentCard 'Saves user's place in cards for when they return
 
        Sheets("WorkingData").Range("B" & r) = PI.Text
