        Sheets("WorkingData").Range("DX" & r) = D5R3Normal.Text
        Sheets("WorkingData").Range("DY" & r) = D5R4Normal.Text
        Sheets("WorkingData").Range("DZ" & r) = D5R1Abnormal.Text
        Sheets("WorkingData").Range("EA" & r) = D5R2Abnormal.Text
        Sheets("WorkingData").Range("EB" & r) = D5R3Abnormal.Text
        Sheets("WorkingData").Range("EC" & r) = D5R4Abnormal.Text
        Sheets("WorkingData").Range("ED" & r) = D5R1DiseasedDead.Text
        Sheets("WorkingData").Range("EE" & r) = D5R2DiseasedDead.Text
        Sheets("WorkingData").Range("EF" & r) = D5R3DiseasedDead.Text
        Sheets("WorkingData").Range("EG" & r) = D5R4DiseasedDead.Text
        Sheets("WorkingData").Range("EH" & r) = D5R1Dormant.Text
        Sheets("WorkingData").Range("EI" & r) = D5R2Dormant.Text
        Sheets("WorkingData").Range("EJ" & r) = D5R3Dormant.Text
        Sheets("WorkingData").Range("EK" & r) = D5R4Dormant.Text
        Sheets("WorkingData").Range("EL" & r) = D5R1Hard.Text
        Sheets("WorkingData").Range("EM" & r) = D5R2Hard.Text
        Sheets("WorkingData").Range("EN" & r) = D5R3Hard.Text
        Sheets("WorkingData").Range("EO" & r) = D5R4Hard.Text
        Sheets("WorkingData").Range("EP" & r) = D6Date.Text
        Sheets("WorkingData").Range("EV" & r) = D6R1Normal.Text
        Sheets("WorkingData").Range("EW" & r) = D6R2Normal.Text
        Sheets("WorkingData").Range("EX" & r) = D6R3Normal.Text
        Sheets("WorkingData").Range("EY" & r) = D6R4Normal.Text
        Sheets("WorkingData").Range("EZ" & r) = D6R1Abnormal.Text
        Sheets("WorkingData").Range("FA" & r) = D6R2Abnormal.Text
        Sheets("WorkingData").Range("FB" & r) = D6R3Abnormal.Text
        Sheets("WorkingData").Range("FC" & r) = D6R4Abnormal.Text
        Sheets("WorkingData").Range("FD" & r) = D6R1DiseasedDead.Text
        Sheets("WorkingData").Range("FE" & r) = D6R2DiseasedDead.Text
        Sheets("WorkingData").Range("FF" & r) = D6R3DiseasedDead.Text
        Sheets("WorkingData").Range("FG" & r) = D6R4DiseasedDead.Text
        Sheets("WorkingData").Range("FH" & r) = D6R1Dormant.Text
        Sheets("WorkingData").Range("FI" & r) = D6R2Dormant.Text
        Sheets("WorkingData").Range("FJ" & r) = D6R3Dormant.Text
        Sheets("WorkingData").Range("FK" & r) = D6R4Dormant.Text
        Sheets("WorkingData").Range("FL" & r) = D6R1Hard.Text
        Sheets("WorkingData").Range("FM" & r) = D6R2Hard.Text
        Sheets("WorkingData").Range("FN" & r) = D6R3Hard.Text
        Sheets("WorkingData").Range("FO" & r) = D6R4Hard.Text
    
        'Calculates Replication Count
        RepCount = 0
        If Sheets("WorkingData").Range("R" & r).Value > "0" Then
            RepCount = RepCount + 1
        End If
        If Sheets("WorkingData").Range("S" & r).Value > "0" Then
            RepCount = RepCount + 1
        End If
        If Sheets("WorkingData").Range("T" & r).Value > "0" Then
            RepCount = RepCount + 1
        End If
        If Sheets("WorkingData").Range("U" & r).Value > "0" Then
            RepCount = RepCount + 1
        End If
        Sheets("WorkingData").Range("FR" & r) = RepCount
    
    'Determine if calculated fields should be shown and refresh screen
        TotalNormalSeed.Text = Sheets("WorkingData").Range("M" & r)
        
        'PercentNormalSeed field
        If TotalSeedPlanted.Value > "0" Then
            PercentNormalSeed.Text = Sheets("WorkingData").Range("N" & r).Value
            Me.PercentNormalSeed.Value = Format(Me.PercentNormalSeed.Value, "0.00%")
        Else
            PercentNormalSeed.Text = "N/A"
        End If
        
        'Duration/Day Count fields
        If Sheets("WorkingData").Range("P" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D1DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("Q" & r) <= 0 Then
            D1DayCount.Text = "N/A"
        Else
            D1DayCount.Text = Sheets("WorkingData").Range("Q" & r) & " days"
        End If
        
        If Sheets("WorkingData").Range("AP" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D2DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("AQ" & r) <= 0 Then
            D2DayCount.Text = "N/A"
        Else
            D2DayCount.Text = Sheets("WorkingData").Range("AQ" & r) & " days"
        End If
        
        If Sheets("WorkingData").Range("BP" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D3DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("BQ" & r) <= 0 Then
            D3DayCount.Text = "N/A"
        Else
            D3DayCount.Text = Sheets("WorkingData").Range("BQ" & r) & " days"
        End If
        
        If Sheets("WorkingData").Range("CP" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D4DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("CQ" & r) <= 0 Then
            D4DayCount.Text = "N/A"
        Else
            D4DayCount.Text = Sheets("WorkingData").Range("CQ" & r) & " days"
        End If
        
        If Sheets("WorkingData").Range("DP" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D5DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("DQ" & r) <= 0 Then
            D5DayCount.Text = "N/A"
        Else
            D5DayCount.Text = Sheets("WorkingData").Range("DQ" & r) & " days"
        End If
        
        If Sheets("WorkingData").Range("EP" & r).Value = "" Or Sheets("WorkingData").Range("F" & r).Value = "" Then
            D6DayCount.Text = "N/A"
        ElseIf Sheets("WorkingData").Range("EQ" & r) <= 0 Then
            D6DayCount.Text = "N/A"
        Else
            D6DayCount.Text = Sheets("WorkingData").Range("EQ" & r) & " days"
        End If

        'Count Total fields
        If Sheets("WorkingData").Range("R" & r).Value > 0 Then
            D1R1Total.Text = Sheets("WorkingData").Range("R" & r)
        Else
            D1R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("S" & r).Value > 0 Then
            D1R2Total.Text = Sheets("WorkingData").Range("S" & r)
        Else
            D1R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("T" & r).Value > 0 Then
            D1R3Total.Text = Sheets("WorkingData").Range("T" & r)
        Else
            D1R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("U" & r).Value > 0 Then
            D1R4Total.Text = Sheets("WorkingData").Range("U" & r)
        Else
            D1R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("AR" & r).Value > 0 Then
            D2R1Total.Text = Sheets("WorkingData").Range("AR" & r)
        Else
            D2R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("AS" & r).Value > 0 Then
            D2R2Total.Text = Sheets("WorkingData").Range("AS" & r)
        Else
            D2R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("AT" & r).Value > 0 Then
            D2R3Total.Text = Sheets("WorkingData").Range("AT" & r)
        Else
            D2R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("AU" & r).Value > 0 Then
            D2R4Total.Text = Sheets("WorkingData").Range("AU" & r)
        Else
            D2R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("BR" & r).Value > 0 Then
            D3R1Total.Text = Sheets("WorkingData").Range("BR" & r)
        Else
            D3R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("BS" & r).Value > 0 Then
            D3R2Total.Text = Sheets("WorkingData").Range("BS" & r)
        Else
            D3R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("BT" & r).Value > 0 Then
            D3R3Total.Text = Sheets("WorkingData").Range("BT" & r)
        Else
            D3R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("BU" & r).Value > 0 Then
            D3R4Total.Text = Sheets("WorkingData").Range("BU" & r)
        Else
            D3R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("CR" & r).Value > 0 Then
            D4R1Total.Text = Sheets("WorkingData").Range("CR" & r)
        Else
            D4R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("CS" & r).Value > 0 Then
            D4R2Total.Text = Sheets("WorkingData").Range("CS" & r)
        Else
            D4R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("CT" & r).Value > 0 Then
            D4R3Total.Text = Sheets("WorkingData").Range("CT" & r)
        Else
            D4R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("CU" & r).Value > 0 Then
            D4R4Total.Text = Sheets("WorkingData").Range("CU" & r)
        Else
            D4R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DR" & r).Value > 0 Then
            D5R1Total.Text = Sheets("WorkingData").Range("DR" & r)
        Else
            D5R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DS" & r).Value > 0 Then
            D5R2Total.Text = Sheets("WorkingData").Range("DS" & r)
        Else
            D5R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DT" & r).Value > 0 Then
            D5R3Total.Text = Sheets("WorkingData").Range("DT" & r)
        Else
            D5R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("DU" & r).Value > 0 Then
            D5R4Total.Text = Sheets("WorkingData").Range("DU" & r)
        Else
            D5R4Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ER" & r).Value > 0 Then
            D6R1Total.Text = Sheets("WorkingData").Range("ER" & r)
        Else
            D6R1Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ES" & r).Value > 0 Then
            D6R2Total.Text = Sheets("WorkingData").Range("ES" & r)
        Else
            D6R2Total.Text = ""
        End If
        If Sheets("WorkingData").Range("ET" & r).Value > 0 Then
            D6R3Total.Text = Sheets("WorkingData").Range("ET" & r)
        Else
            D6R3Total.Text = ""
        End If
        If Sheets("WorkingData").Range("EU" & r).Value > 0 Then
            D6R4Total.Text = Sheets("WorkingData").Range("EU" & r)
        Else
            D6R4Total.Text = ""
        End If

    'Makes certain export functions available to access
    If Sheets("RESOURCEDATABASE_Export").Range("B2") = "" Then
        Sheets("RESOURCEDATABASE_Export").Range("B2") = 1
    End If
    If Sheets("Technician_Export").Range("B6") = "" Then
        Sheets("Technician_Export").Range("B6") = 1
    End If

    'Turns back off worksheets this form needed
    Sheets("WorkingData").Visible = False
    Sheets("RESOURCEDATABASE_Export").Visible = False
    Sheets("Technician_Export").Visible = False
    Sheets("ProgramStorage").Visible = False
    Application.ScreenUpdating = True
    
    'Saves Workbook
    
        'Hide all worksheets in workbook except "Start" to make sure macros are enabled
        Dim ws As Worksheet
        Sheets("START").Visible = xlSheetVisible 'Make sure the "Start" worksheet is visible
 
        'Hide all other worksheets
        For Each ws In ThisWorkbook.Worksheets
            If ws.Name <> "START" Then
                ws.Visible = xlVeryHidden
            End If
        Next ws
    
    ActiveWorkbook.Save
    
    'Closes the workbook and Excel as well if only workbook open
    If Workbooks.Count < 2 Then
        Application.Quit
    Else
        ThisWorkbook.Close
    End If

End Sub
