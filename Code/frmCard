        Sheets("WorkingData").Range("DD" & r) = D4R1DiseasedDead.Text
        Sheets("WorkingData").Range("DE" & r) = D4R2DiseasedDead.Text
        Sheets("WorkingData").Range("DF" & r) = D4R3DiseasedDead.Text
        Sheets("WorkingData").Range("DG" & r) = D4R4DiseasedDead.Text
        Sheets("WorkingData").Range("DH" & r) = D4R1Dormant.Text
        Sheets("WorkingData").Range("DI" & r) = D4R2Dormant.Text
        Sheets("WorkingData").Range("DJ" & r) = D4R3Dormant.Text
        Sheets("WorkingData").Range("DK" & r) = D4R4Dormant.Text
